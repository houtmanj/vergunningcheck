{
    "name": "vergunningschecker",
    "version": "0.1.16",
    "description": "Amsterdam Vergunningschecker",
    "repository": {
        "type": "git",
        "url": "https://github.com/Amsterdam/Vergunningschecker"
    },
    "engines": {
        "npm": ">=5",
        "node": ">=11.10.1"
    },
    "author": "Datapunt Amsterdam",
    "license": "MPL-2.0",
    "scripts": {
        "analyze": "node ./internals/scripts/analyze.js",
        "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
        "preinstall": "npm run npmcheckversion",
        "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
        "build:sttr:staging": "rm ./app/shared/content/sttr/*; STTR_ENV=staging node sttr_build --output=./app/shared/content/sttr",
        "build:sttr:prod": "rm ./app/shared/content/sttr/*; STTR_ENV=production node sttr_build --output=./app/shared/content/sttr",
        "build:stats": "webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --display-optimization-bailout --json > stats.json",
        "start": "cross-env NODE_ENV=development node server",
        "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
        "start:production": "npm run test && npm run build && npm run start:prod",
        "start:prod": "cross-env NODE_ENV=production node server",
        "presetup": "npm i chalk shelljs",
        "setup": "node ./internals/scripts/setup.js",
        "clean": "shjs ./internals/scripts/clean.js",
        "lint": "npm run lint:js",
        "lint:css": "stylelint './app/**/*.js'",
        "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals --ignore-pattern server",
        "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals --ignore-pattern server --fix",
        "lint:js": "npm run lint:eslint -- . ",
        "lint:staged": "lint-staged",
        "pretest": "npm run test:clean && npm run lint",
        "test:clean": "rm -rf ./coverage",
        "test": "cross-env NODE_ENV=test jest --coverage",
        "test:watch": "cross-env NODE_ENV=test jest --watchAll",
        "prettify": "prettier --write",
        "build:acc": "cross-env NODE_ENV=acceptance webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
        "build:prod": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
        "release": "standard-version"
    },
    "browserslist": [
        "last 2 versions",
        "> 1%",
        "IE 10"
    ],
    "lint-staged": {
        "*.js": [
            "npm run lint:eslint:fix",
            "git add --force"
        ],
        "*.json": [
            "prettier --write",
            "git add --force"
        ]
    },
    "pre-commit": "lint:staged",
    "resolutions": {
        "babel-core": "7.0.0-bridge.0"
    },
    "dependencies": {
        "@babel/polyfill": "7.8.3",
